# PostgreSQL database credentials
POSTGRES_USER=your_postgres_user
POSTGRES_PASSWORD=your_postgres_password
POSTGRES_DB=your_database_name

# SQLAlchemy database URL (used by the backend)
DATABASE_URL=postgresql://your_postgres_user:your_postgres_password@db:5432/your_database_name

# Redis connection URL (used for Celery broker and backend)
REDIS_URL=redis://redis:6379/0

# External API for user info (template, keep trailing slash)
USERS_API_URL_TEMPLATE=https://drones-api.hive.fi/users/

# Secret key for accessing NFZ endpoints
NFZ_SECRET_KEY=your_secret_key
